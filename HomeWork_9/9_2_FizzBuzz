selected_number = int(input("Select a number between 1 and 100: "))

for x in range(1, selected_number + 1):
    remainder_3 = (x % 3)
    remainder_5 = (x % 5)

    if remainder_3 == 0 and remainder_5 == 0:
        print("fizzbuzz")
    elif remainder_3 == 0:
        print("fizz")
    elif remainder_5 == 0:
        print("buzz")
    else:
        print(x)